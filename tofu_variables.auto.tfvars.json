{
  "smtp_from_name": "Keycloak",
  "keycloak_roles": {
    "admin": "A user with this role should be provided access to admin panels for managing services.",
    "trusted": "A user with this role may be provided access to more services than a default user."
  },
  "keycloak_group_roles": {
    "admin": [
      "admin",
      "trusted"
    ],
    "trusted": [
      "trusted"
    ]
  },
  "oauth2_clients": {
    "traefik-dashboard": {
      "mfa": true,
      "service": "api@internal",
      "roles": [
        "admin"
      ],
      "traefik_path_condition": "(Path(`/`)||PathPrefix(`/api`)||PathPrefix(`/dashboard`))"
    },
    "whoami": {
      "mfa": false
    },
    "whoami-admin": {
      "mfa": true,
      "roles": [
        "admin"
      ]
    },
    "vaultwarden": {
      "mfa": true,
      "roles": [
        "trusted"
      ]
    },
    "forgejo": {
      "mfa": true,
      "roles": [
        "trusted"
      ]
    }
  }
}